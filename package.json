{
  "name": "excel-row-stream",
  "version": "1.0.0",
  "description": "Memory efficinet streaming XLSX parser",
  "main": "buid/index.js",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "rollup -c",
    "compile": "tsc --project tsconfig.json --noEmit",
    "compile:watch": "npm run compile -- --watch",
    "lint": "eslint .",
    "test": "NODE_ENV=test jest --verbose",
    "test:coverage": "NODE_ENV=test jest --verbose --coverage",
    "test:watch": "NODE_ENV=test jest --verbose --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/velocityzen/excel-row-stream.git"
  },
  "keywords": [
    "excel",
    "xlsx",
    "reader",
    "stream",
    "simple",
    "parser"
  ],
  "author": "velocityzen <v@2dubs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/velocityzen/excel-row-stream/issues"
  },
  "homepage": "https://github.com/velocityzen/excel-row-stream#readme",
  "dependencies": {
    "@types/unzipper": "^0.10.5",
    "fp-ts": "^2.12.1",
    "htmlparser2": "^8.0.1",
    "ssf": "^0.11.2",
    "tmp": "^0.2.1",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "rollup-plugin-node-externals": "^4.0.0",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/tests"
    ],
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    }
  }
}
